      "itemSelector": ".agency-card",
      "fields": {
        "card_title": ".card-title",
        "card_tags": ".tag"
      }
    },
    "team_members": {
      "itemSelector": ".team-card",
      "fields": {
        "member_name": ".team-name",
        "member_role": ".team-role"
      }
    },
    "social_proof_text": {
      "itemSelector": ".social-proof-text"
    },
    "faqs": {
      "itemSelector": ".faq-item",
      "fields": {
        "faq_question": ".faq-question",
        "faq_answer": ".faq-answer"
      }
    },
    "final_cta_paragraphs": {
      "itemSelector": ".final-cta-left p"
    },
    "final_cta_features": {
      "itemSelector": ".final-cta-left li"
    },
    "footer_columns": {
      "itemSelector": ".footer-column",
      "fields": {
        "column_title": "h4",
        "column_links": "a"
      }
    },
    "footer_tools": {
      "itemSelector": ".footer-tool"
    }
  }
