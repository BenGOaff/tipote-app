  "arrays": {
    "nav_links": {
      "itemSelector": ".nav-links a"
    },
    "hero_badges": {
      "itemSelector": ".badge"
    },
    "tools": {
      "itemSelector": ".tool-item",
      "fields": {
        "tool_name": ".tool-name",
        "tool_description": ".tool-description"
      }
    },
    "value_paragraphs": {
      "itemSelector": ".value-text"
    },
    "content_modules": {
      "itemSelector": ".content-module",
      "fields": {
        "module_title": ".module-title",
        "module_description": ".module-description",
        "module_features": ".module-features li",
        "module_highlight": ".module-highlight p"
      }
    },
    "agency_paragraphs": {
      "itemSelector": ".agency-text p"
    },
