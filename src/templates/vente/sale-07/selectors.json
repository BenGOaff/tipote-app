  "arrays": {
    "problem_intro_paragraphs": {
      "itemSelector": ".problem-intro p"
    },
    "platforms": {
      "itemSelector": ".platform-item",
      "fields": {
        "platform_icon": ".platform-icon",
        "platform_title": ".platform-content h3",
        "platform_description": ".platform-content p"
      }
    },
    "story_paragraphs": {
      "itemSelector": ".story p"
    },
    "solution_paragraphs": {
      "itemSelector": ".solution-intro p"
    },
    "advantages": {
      "itemSelector": ".advantage-card",
      "fields": {
        "advantage_icon": ".advantage-icon",
        "advantage_title": "h3",
        "advantage_description": "p"
      }
    },
    "benefits": {
      "itemSelector": ".benefit-item",
      "fields": {
        "benefit_icon": ".benefit-icon",
        "benefit_title": "h4",
        "benefit_description": "p"
      }
    },
    "comparison_note_paragraphs": {
      "itemSelector": ".comparison-note p"
    },
    "comparison_charts": {
      "itemSelector": ".comparison-card h3"
    },
    "cake_labels": {
      "itemSelector": ".cake-item h4"
    },
    "modules": {
      "itemSelector": ".module-item",
      "fields": {
        "module_title": "h3",
        "module_description": "p"
      }
    },
    "bonuses": {
      "itemSelector": ".bonus-card",
      "fields": {
        "bonus_title": ".bonus-content h3",
        "bonus_subtitle": ".bonus-content h4",
        "bonus_paragraphs": ".bonus-content p"
      }
    },
    "faqs": {
      "itemSelector": ".faq-item"
    },
    "testimonials": {
      "itemSelector": ".testimonial-card",
      "fields": {
        "testimonial_paragraphs": ".testimonial-text"
      }
    },
    "author_paragraphs": {
      "itemSelector": ".author-content p"
    },
    "recap_list": {
      "itemSelector": ".recap-list li"
    },
    "footer_links": {
      "itemSelector": ".footer-links a"
    }
  }
