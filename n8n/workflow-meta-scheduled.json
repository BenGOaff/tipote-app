            {
              "id": "check-ig-posts",
              "leftValue": "={{ $json.count }}",
              "rightValue": 0,
              "operator": {
                "type": "number",
                "operation": "gt"
              }
            }
          ],
          "combinator": "and"
        }
      },
      "id": "has-ig-posts",
      "name": "Has IG Posts?",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [690, 420]
    },
